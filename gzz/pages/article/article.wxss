.db {
  width: 100%;
  padding-bottom: 2rpx;
  border-top: 1px solid #eee;
}

.db .header {
  width: 92%;
  margin: 0 auto;
}

.db .header .header_title {
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(33, 33, 33, 1);
  padding-top: 18rpx;
}

.db .header .header_tiem {
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
}

.db .header .header_tiem .tiem {
  font-size: 22rpx;
  color: rgba(153, 153, 153, 1);
  height: 41rpx;
  line-height: 41rpx;
  text-align: center;
}

.db .header .header_tiem image {
  width: 122rpx;
  height: 41rpx;
}

.db .header .text {
  width: 100%;
  margin-top: 36rpx;
}

.db .header .text .text_wz {
  font-size: 28rpx;
  color: rgba(102, 102, 102, 1);
}

.db .header .text image {
  margin: 21rpx 0 30rpx 0;
  width: 670rpx;
  height: 442rpx;
}
